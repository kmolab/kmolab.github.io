cursor = context.scene.cursor_location

radialdist = 2.0

xsize = 0.15
ysize = 0.15
zsize = 0.15

theta = 0.0
pi_over_8 = 6.28 / 16.0

levels = -10
maxlevels = 10
divi = 2.0
